							<button mode='mode_shoutcast' class="button" >			
									<buttonImg class="buttonImg" src='Icons/ShoutCast.png' alt='음악방송(shoutcast)'/>	
									<buttonName class="buttonText" name='음악방송' />
							</button>
							<button mode='mode_music' class="button" >								
									<buttonImg class="buttonImg" src='Icons/Music.png' alt='음악앨범' />	
									<buttonName class="buttonText" name='음악앨범' />
							</button>
							<button mode='mode_whitenoise' class="button" >			
									<buttonImg class="buttonImg" src='Icons/WhiteNoise.png' alt='백색소음'/>	
									<buttonName class="buttonText" name='백색소음' />
							</button>
-->
			</page>

</pageGroup>
